{
  "extends": [
    "airbnb"
  ],
  "env": {
    "mocha": true,
    "browser": true
  },
  "rules": {
    "object-curly-spacing": [
      "error",
      "never"
    ],
    "prefer-template": [
      "off"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "no-mixed-operators": [
      "off"
    ],
    "no-param-reassign": [
      "off"
    ],
    "no-multi-spaces": [
      "off"
    ],
    "func-names": [
      "off"
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "arrow-body-style": [
      "off"
    ],
    "consistent-return": [
      "off"
    ],
    "no-console": [
      "off"
    ],
    "no-use-before-define": [
      "error",
      {
        "variables": false
      }
    ],
    "newline-per-chained-call": [
      "off"
    ],
    "no-shadow": [
      "off"
    ],
    "no-else-return": [
      "off"
    ],
    "no-useless-escape": [
      "off"
    ],
    "no-prototype-builtins": [
      "off"
    ],
    "no-plusplus": [
      "off"
    ],
    "no-useless-constructor": [
      "off"
    ],
    "react/forbid-prop-types": [
      "off"
    ],
    "react/no-array-index-key": [
      "off"
    ],
    "react/no-did-mount-set-state": [
      "off"
    ]
  }
}
